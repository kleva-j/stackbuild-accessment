{
  "name": "stackbuild-accessment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:migrate": "prisma migrate deploy",
    "build:generate": "prisma generate",
    "build:next": "next build",
    "dev": "next dev --experimental-https",
    "build": "run-s build:*",
    "build-dev": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "studio": "prisma studio",
    "dx": "run-s db-up db-migrate db-generate dev",
    "db-pull": "prisma db pull",
    "db-up": "docker-compose up -d",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "db-migrate-dev": "prisma migrate dev --name init",
    "migrate": "prisma migrate dev --name emailVerified",
    "db-migrate": "prisma migrate deploy",
    "db-generate": "prisma generate",
    "db-seed": "prisma db seed",
    "db-setup": "pnpm db-pull",
    "db-format": "prisma format",
    "turso-sync-migration": "pnpm dlx prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma --script > prisma/migration.sql",
    "turso-db-migrate": "turso db shell stb-test < prisma/migration.sql"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.27.1",
    "@editorjs/code": "^2.9.0",
    "@editorjs/editorjs": "^2.28.2",
    "@editorjs/embed": "^2.7.0",
    "@editorjs/header": "^2.8.1",
    "@editorjs/inline-code": "^1.5.0",
    "@editorjs/link": "^2.6.2",
    "@editorjs/list": "^1.9.0",
    "@editorjs/paragraph": "^2.11.3",
    "@editorjs/table": "^2.3.0",
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "5.6.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^2.0.1",
    "@tanstack/react-query": "^4.36.1",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "contentlayer": "^0.3.4",
    "editorjs-react-renderer": "^3.5.1",
    "lucide-react": "^0.292.0",
    "next": "14.0.3",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-headless-pagination": "^1.1.4",
    "react-hook-form": "^7.48.2",
    "react-textarea-autosize": "^8.5.3",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@faker-js/faker": "^8.3.1",
    "@types/mdx": "^2.0.10",
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "**/*.+(js|jsx|ts|tsx)": [
      "pnpm format"
    ]
  },
  "packageManager": "pnpm@8.8.0"
}
