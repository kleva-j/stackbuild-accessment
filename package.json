{
  "name": "stackbuild-accessment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "studio": "prisma studio",
    "dx": "run-s db-migrate-dev db-seed dev",
    "db-pull": "prisma db pull",
    "db-migrate-dev": "prisma migrate dev",
    "db-migrate": "prisma migrate deploy",
    "db-generate": "prisma generate",
    "db-seed": "prisma db seed",
    "db-setup": "pnpm db-pull",
    "db-format": "prisma format",
    "turso-sync-migration": "pnpm dlx prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma --script > prisma/migration.sql",
    "turso-db-migrate": "turso db shell stb-test < prisma/migration.sql"
  },
  "dependencies": {
    "@libsql/client": "^0.3.5",
    "@prisma/adapter-libsql": "^5.4.1",
    "@prisma/client": "5.4.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "contentlayer": "^0.3.4",
    "next": "13.5.4",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/mdx": "^2.0.8",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "prisma": "^5.4.1",
    "tailwindcss": "^3",
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*.+(js|jsx|ts|tsx)": [
      "pnpm format"
    ]
  },
  "packageManager": "pnpm@8.8.0"
}
