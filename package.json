{
  "name": "stackbuild-accessment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:migrate": "prisma migrate deploy",
    "build:generate": "prisma generate",
    "build:next": "next build",
    "dev": "next dev --experimental-https",
    "build": "run-s build:*",
    "build-dev": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "studio": "prisma studio",
    "dx": "run-s db-up db-migrate db-generate dev",
    "db-pull": "prisma db pull",
    "db-up": "docker-compose up -d",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "db-migrate-dev": "prisma migrate dev --name init",
    "migrate": "prisma migrate dev --name emailVerified",
    "db-migrate": "prisma migrate deploy",
    "db-generate": "prisma generate",
    "db-seed": "prisma db seed",
    "db-setup": "pnpm db-pull",
    "db-format": "prisma format",
    "turso-sync-migration": "pnpm dlx prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma --script > prisma/migration.sql",
    "turso-db-migrate": "turso db shell stb-test < prisma/migration.sql"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.3",
    "@editorjs/code": "^2.8.0",
    "@editorjs/editorjs": "^2.28.0",
    "@editorjs/embed": "^2.5.3",
    "@editorjs/header": "^2.7.0",
    "@editorjs/inline-code": "^1.4.0",
    "@editorjs/link": "^2.5.0",
    "@editorjs/list": "^1.8.0",
    "@editorjs/paragraph": "^2.10.0",
    "@editorjs/table": "^2.2.2",
    "@hookform/resolvers": "^3.3.1",
    "@libsql/client": "^0.3.5",
    "@prisma/adapter-libsql": "^5.4.1",
    "@prisma/client": "5.4.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^2.0.0",
    "@tanstack/react-query": "^4.36.1",
    "axios": "^1.5.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "contentlayer": "^0.3.4",
    "editorjs-react-renderer": "^3.5.1",
    "lucide-react": "^0.284.0",
    "next": "13.5.4",
    "next-auth": "^4.23.2",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.47.0",
    "react-textarea-autosize": "^8.5.3",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@faker-js/faker": "^8.1.0",
    "@types/mdx": "^2.0.8",
    "@types/node": "^20.8.3",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "prisma": "^5.4.1",
    "tailwindcss": "^3",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "**/*.+(js|jsx|ts|tsx)": [
      "pnpm format"
    ]
  },
  "packageManager": "pnpm@8.8.0"
}
